\n  })]), _createElementVNode(\"div\", _hoisted_7, [_createVNode(_component_el_card, {\n    class: \"activity-card\"\n  }, {\n    default: _withCtx(() => [_createElementVNode(\"div\", _hoisted_8, [_cache[10] || (_cache[10] = _createElementVNode(\"h2\", null, \"我的编程日历\", -1)), _createVNode(_component_el_select, {\n      modelValue: $data.timeRange,\n      \"onUpdate:modelValue\": _cache[3] || (_cache[3] = $event => $data.timeRange = $event),\n      size: \"small\",\n      style: {\n        \"width\": \"120px\"\n      }\n    }, {\n      default: _withCtx(() => [_createVNode(_component_el_option, {\n        label: \"最近半年\",\n        value: \"half_year\"\n      }), _createVNode(_component_el_option, {\n        label: \"最近一年\",\n        value: \"one_year\"\n      }), _createVNode(_component_el_option, {\n        label: \"全部\",\n        value: \"all\"\n      })]),\n      _: 1\n    }, 8, [\"modelValue\"])]), _createElementVNode(\"div\", _hoisted_9, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($data.heatmapTabs, tab => {\n      return _openBlock(), _createElementBlock(\"div\", {\n        key: tab.id,\n        class: _normalizeClass(['heatmap-tab', {\n          active: $data.activeTab === tab.id\n        }]),\n        onClick: $event => $data.activeTab = tab.id\n      }, _toDisplayString(tab.label), 11, _hoisted_10);\n    }), 128))]), _createElementVNode(\"div\", _hoisted_11, [_createElementVNode(\"div\", _hoisted_12, [_createElementVNode(\"div\", _hoisted_13, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($options.monthLabels, (month, index) => {\n      return _openBlock(), _createElementBlock(\"div\", {\n        key: index,\n        class: \"month-label\",\n        style: _normalizeStyle({\n          gridColumnStart: month.columnStart\n        })\n      }, _toDisplayString(month.label), 5);\n    }), 128))]), _createElementVNode(\"div\", _hoisted_14, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($options.c